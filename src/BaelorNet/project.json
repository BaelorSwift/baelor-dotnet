{
	"version": "2.0.1",
	"title": "Baelor.Net",
	"description": "Api client library for https://www.baelor.io/",
	"authors": [ "Alex Forbes-Reed" ],
	"packOptions": {
		"tags": [ "Baelor", "Taylor Swift", "Bae" ],
		"projectUrl": "https://github.com/BaelorSwift/baelor-dotnet",
		"repository": {
			"type": "git",
			"url": "https://github.com/BaelorSwift/baelor-dotnet"
		}
	},

	"dependencies": {
		"Newtonsoft.Json": "9.0.1"
	},

	"frameworks": {
		"netstandard1.6": {
			"dependencies": {
				"NETStandard.Library": "1.6.0"
			},
			"imports": [
				"dnxcore50",
				"portable-net451+win8+win81+win10"
			]
		}
	},

	"buildOptions": {
		"debugType": "portable"
	}
}
